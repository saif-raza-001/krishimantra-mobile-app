        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#4CAF50"
      },
      "package": "com.yourdomain.krishimantra"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "26578e10-ced4-4865-9df8-04fe3d2e71c9"
      }
    },
    "plugins": [
      "expo-font"
    ]
  }
}
